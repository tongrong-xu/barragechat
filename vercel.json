{
  "routes": [
    { "src": "^/static/(.*)", "dest": "/public/$1" },
    { "src": "/api/(.*)", "dest": "/app.js" },
    { "src": "/room/Room_([a-zA-Z0-9_-]+)", "dest": "/UserRoute.js" },
    { "src": "/room/joinClassroom", "dest": "/UserRoute.js" },
    { "src": "/room/GoChat", "dest": "/UserRoute.js" },
    { "src": "/loginstudent", "dest": "/UserRoute.js" },
    { "src": "/registerstudent", "dest": "/UserRoute.js" },
    { "src": "/loginteacher", "dest": "/UserRoute.js" },
    { "src": "/registerteacher", "dest": "/UserRoute.js" },
    { "src": "/logout", "dest": "/UserRoute.js" },
    { "src": "/home", "dest": "/UserRoute.js" },
    { "src": "/(.*)", "dest": "/UserRoute.js" }
  ]
}